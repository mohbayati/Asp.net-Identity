@{
}
@using Asp.net_Identity.Model;
@model IEnumerable<AppUser>
<table class="table table-sm table-bordered">
    <tr>
        <th>
            ID
        </th>
        <th>
            Name
        </th>
        <th>
            Email
        </th>
        
    </tr>
    <tr>
        @foreach (var user in Model)
        {
            <td>
                @user.Id
            </td>
            <td>@user.UserName</td>
            <td>@user.Email</td>
        }
    </tr>
</table>